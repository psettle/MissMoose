<UserControl x:Class="MissMooseConfigurationApplication.UIComponents.LineWithBorder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MissMooseConfigurationApplication.UIComponents"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Line Name="BorderLine" X1="0" X2="0" Y1="0" Y2="0" Stroke="Black" StrokeEndLineCap="Round" StrokeStartLineCap="Round"/>
        <Line Name="ColoredLine" X1="0" X2="0" Y1="0" Y2="0" Stroke="Black" StrokeEndLineCap="Round" StrokeStartLineCap="Round"/>
    </Grid>
</UserControl>
