<DockPanel x:Class="MissMooseConfigurationApplication.PageSwitcherTextbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MissMooseConfigurationApplication"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Margin="4">
    <Button DockPanel.Dock="Top" Name="TitleButton" Background="Gray" Foreground="White" BorderThickness="2" BorderBrush="Black" Click="PageSwitchClick"></Button>
    <ScrollViewer DockPanel.Dock="Top" VerticalScrollBarVisibility="Auto" Background="White">
        <ItemsControl Name="TextItems" BorderThickness="2,0,2,2" BorderBrush="Black"></ItemsControl>
    </ScrollViewer>
</DockPanel>
