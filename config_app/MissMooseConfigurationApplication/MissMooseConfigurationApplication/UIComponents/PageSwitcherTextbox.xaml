<DockPanel x:Class="MissMooseConfigurationApplication.PageSwitcherTextbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MissMooseConfigurationApplication"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Margin="4">
    <Button DockPanel.Dock="Top" Name="TitleButton" FontSize="14" Background="{StaticResource ThemeBrushForeground}" Foreground="{StaticResource ThemeBrushText}" BorderThickness="0" Click="PageSwitchClick"></Button>
    <ListBox Name="TextList" DockPanel.Dock="Top" Background="{StaticResource ThemeBrushBackground}" BorderThickness="0"
             ScrollViewer.HorizontalScrollBarVisibility="Disabled" HorizontalContentAlignment="Stretch"/>
</DockPanel>
